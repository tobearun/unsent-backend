{
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/gemini.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api",
      "dest": "api/index.ts"
    },
    {
      "src": "/api/gemini",
      "dest": "api/gemini.js"
    }
  ]
}
